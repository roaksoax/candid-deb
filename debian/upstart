description "TheBlues Identity Service"
author "Juju Team <juju@lists.ubuntu.com>"
start on runlevel [2345]
stop on runlevel [!2345]
respawn

setuid candid
setgid candid

pre-start script
    if [ ! -r /etc/candid/config.yaml ];then
        stop; exit 0;
    fi
end script
exec candidsrv /etc/candid/config.yaml
